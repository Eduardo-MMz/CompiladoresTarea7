-----------------------------------------------------------------------------------

1) x = z+2*8-(a/b)-x
Código intermedio:

t0 = 2*8
t1 = a/b
t2 = z + t0
t3 = t2 - t1
t4 = t3 - x
x = t4

-----------------------------------------------------------------------------------

2) int x[6]; x[j+j*2] = a*3/6;
Código intermedio:

t0 = a*3
t1 = t0/6
t2 = j*2
t3 = j + t2
t4 = t3 * 4
x[t4] = t1

-----------------------------------------------------------------------------------

3) int x[6], y[5][3][2];  x[j+j*2]=y[i+2][j-1][k*3]
Código intermedio:

t0 = i+2
t1 = t0 * 24
t2 = j-1
t3 = t2 * 8
t4 = t1 + t3
t5 = k*3
t6 = t4 * 4
t7 = t4 + t6
t8 = j*2
t9 = j + t8
t10 = t9 * 4
t11 = y[t7]
x[t10] = t11

-----------------------------------------------------------------------------------

4) if(x[i+2] > x[i-1]) a=z*3+y; else a=2;
Código intermedio:

t0 = i+2
t1 = t0 * 4
t2 = i-1
t3 = t2 * 4
t4 = x[t1]
t5 = x[t3]
if t4 > t5 goto L0
    goto L1
    
L0: t6 = z*3
    a = t6 + y
    
L1: a = 2

-----------------------------------------------------------------------------------

5) while(x[i+2] > x[i-1]){
      if(y<3)  y=1;
      else  y=0;
   }
Código intermedio:
   
t0 = i+2
t1 = t0 * 4
t2 = i-1
t3 = t2 * 4
t4 = x[t1]
t5 = x[t3]

L0: if t4 > t5 goto L1
    goto L2

L1: if y < 3 goto L3
    goto L4

L3: y=1 goto L0
L4: y=0 goto L0

L2: ...

-----------------------------------------------------------------------------------

6) do
     i++;
     j++;
  while(x > z*5);
Código intermedio:
  
L0: t0 = i+1
    i = t0
    t1 = j+1
    j = t1
    t2 = z*5
    if x > t2 goto L0
        goto L1
    
L1: ...

